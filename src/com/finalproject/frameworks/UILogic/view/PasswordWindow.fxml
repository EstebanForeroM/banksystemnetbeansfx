         </image>
      </ImageView>
      <Button fx:id="enterToTransactions" layoutX="189.0" layoutY="267.0" mnemonicParsing="false" onAction="#handleenterToTransactions" prefHeight="40.0" prefWidth="120.0" text="Enter" />
